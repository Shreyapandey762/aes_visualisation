<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.aes_visualisation">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Aes_visualisation"
        tools:targetApi="31">

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".StateWordActivity"
            android:label="State Word Matrix" />

        <!-- Add more activities, services, receivers, etc. as needed -->

        <activity android:name=".CircularShiftActivity"/>
        <activity android:name=".SBoxActivity"/>
        <activity android:name=".RConstActivity"/>
        <activity android:name=".XORInputActivity"/>
        <activity android:name=".ResKeyActivity"/>
        <activity android:name=".SubBoxActivity"/>
        <activity android:name=".ShiftRowsActivity"/>
        <activity android:name=".MixColumnsActivity"/>
        <activity android:name=".AddRoundKeyActivity"/>
        <activity android:name= ".ResultActivity"/>
        <activity android:name= ".ExActivity"/>
    </application>

</manifest>



